{
    "name": "BFWS -- FF-parser version",
    "shortname": "bfws-ff",
    "description": "This package contains the FF parser version of the Best First Width Search planner.",
    "install-size": "0M",
    "dependencies": [
        "lapkt"
    ],
    "endpoint": {
        "services": {
            "solve": {
                "args": [
                    {
                        "name": "domain",
                        "type": "file",
                        "description": "domain file"
                    },
                    {
                        "name": "problem",
                        "type": "file",
                        "description": "problem file"
                    }
                ],
                "call": "dual-bfws-ffparser {domain} {problem} plan",
                "return": {
                    "type": "generic",
                    "files": "*plan*"
                }
            }
        }
    }
}